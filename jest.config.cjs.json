{
  "verbose": true,
  "collectCoverage": true,
  "collectCoverageFrom": [
      "**/src/*.cjs",
      "!**/dist/**",
      "!**/*.config.js",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/_sandbox/**",
      "!**/_recycle/**"
  ],
  "coverageDirectory": "./test/coverage/cjs",
  "testMatch": [
    "**/test/*.cjs"
  ],
  "testPathIgnorePatterns": ["/node_modules/"],
  "testTimeout": 120000
}